{
  "name": "ts-lib",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --fix",
    "commit": "gz-cli git-commit",
    "esno": "esno",
    "update-pkg": "ncu --deep -u",
    "update-version": "bumpp package.json",
    "publish-pkg": "pnpm -r publish --access public",
    "release": "pnpm update-version && pnpm publish-pkg"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "execa": "7.1.1",
    "kolorist": "^1.7.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@gz/front-end-cli": "workspace:*",
    "@types/prompts": "^2.4.3",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "eslint-config-soybeanjs": "0.3.1",
    "esno": "^0.16.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  }
}
