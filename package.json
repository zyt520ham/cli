{
  "name": "@gz/front-end-cli",
  "version": "0.1.7",
  "private": true,
  "description": "web command lint tools",
  "license": "",
  "main": "dist/index.mjs",
  "bin": {
    "gz-cli": "dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "base-bootstrap": "pnpm install --filter @gz/front-end-cli",
    "bootstrap": "pnpm install -w",
    "re-install": "pnpm rimraf node_modules package-lock.json yarn.lock pnpm-lock.yaml ./**/node_modules ./**/package-lock.json ./**/yarn.lock ./**/pnpm-lock.yaml && pnpm bootstrap ",
    "commit": "gz-cli git-commit",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "updatepkg": "gz-cli update-pkg",
    "update-version": "bumpp package.json",
    "build": "tsup",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "execa": "5.1.1",
    "kolorist": "^1.6.0",
    "minimist": "^1.2.7",
    "npm-check-updates": "^16.6.2",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "zx": "^7.1.1"
  },
  "devDependencies": {
    "@gz/front-end-cli": "workspace:*",
    "@types/prompts": "^2.4.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "eslint-config-soybeanjs": "0.2.1",
    "esno": "^0.16.3",
    "lint-staged": "^13.1.0",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm gz-cli git-commit-verify",
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  },
  "publishConfig": {
    "registry": ""
  }
}
